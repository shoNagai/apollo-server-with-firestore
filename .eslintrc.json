{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "no-console": "off",
    "@typescript-eslint/ban-types": [
      "error",
      {
        "types": {
          // for graphql codegen
          "{}": false
        },
        "extendDefaults": false
      }
    ]
  }
}
